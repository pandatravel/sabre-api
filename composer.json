{
    "name": "ammonkc/sabre-api",
    "description": "Sabre API v1 Client",
    "version": "0.1.0",
    "type": "library",
    "keywords": ["sabre", "panda", "api"],
    "license": "UNLICENSED",
    "homepage": "https://github.com/ammonkc/sabre-api",
    "authors": [
        {
            "name": "Ammon Casey",
            "email": "acasey@panda-group.com",
            "role" : "Lead Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.2",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Ammonkc\\Sabre\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ammonkc\\Sabre\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
